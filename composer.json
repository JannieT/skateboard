{
    "name": "skateboard/skateboard",
    "description": "Skateboard web app",
    "type": "project",
    "keywords": [
        "framework",
        "skateboard"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "skateboard/framework": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
